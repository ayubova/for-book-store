{
    "parser": "babel-eslint",
    "extends": ["airbnb-base", "eslint:recommended", "prettier", "plugin:react/recommended"],
    "plugins": ["prettier", "react"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true, 
          "trailingComma": "all"
        }
      ],
      "react/prop-types": [1],
      "no-console": "off"
    }
  }